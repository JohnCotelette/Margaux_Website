        <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
        <meta name="format-detection" content="telephone=no">
        <meta name="description" content="Margaux Passot, 25 ans, directrice artistique basée à Lille, France.">
        <link rel="shortcut icon" type="image/png" href="{{ asset("build/images/smile_black.4c958b7a.png") }}">

        <title>
